			<xs:restriction base="options">
				<xs:maxInclusive value="4"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Screens" type="nxListType">
		<xs:annotation>
			<xs:documentation>ID13071</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Commands" type="nxListType">
