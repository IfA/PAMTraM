							<xs:documentation>points of a "poly"-object</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="Mechanic">
		<xs:annotation>
			<xs:documentation>Button settings</xs:documentation>
