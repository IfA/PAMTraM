<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.eclipse.ui.Examples/de.mfreund.pamtram.examples.category"
            class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
            finalPerspective="de.mfreund.pamtram.pamtramPerspective"
            icon="icons/project.png"
            id="de.mfreund.pamtram.examples.autoprobe"
            name="AutoProBe"
            project="true">
      </wizard>
      <wizard
            category="org.eclipse.ui.Examples/de.mfreund.pamtram.examples.category"
            class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard"
            finalPerspective="de.mfreund.pamtram.pamtramPerspective"
            icon="icons/project.png"
            id="de.mfreund.pamtram.examples.w3caui-movisa"
            name="W3CAUI -&gt; Movisa"
            project="true">
      </wizard>
      <wizard
