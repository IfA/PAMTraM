			<xs:enumeration value="end" />
			<xs:enumeration value="middle-start" />
			<xs:enumeration value="middle-end" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="angleType">
		<xs:annotation>
			<xs:documentation>angle 0-359Â°</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedShort">
			<xs:maxInclusive value="359" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="formatType">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="((x.)?x{1,5}|x,(x){2,3}(.(x){1,3})?)?" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="signedMillion">
		<xs:annotation>
