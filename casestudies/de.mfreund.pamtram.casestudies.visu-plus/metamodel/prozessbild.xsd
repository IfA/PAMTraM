	<xs:simpleType name="tagName">
		<xs:restriction base="xs:string">
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="screenName">
		<xs:restriction base="xs:string">
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="imageFile">
		<xs:restriction base="xs:string">
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="colorValueType1">
		<xs:restriction base="xs:unsignedInt">
			<xs:maxInclusive value="16777215"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="colorValueType2">
		<xs:annotation>
			<xs:documentation>color value: number between 0 and 255 for primary
				colors "rgb(valueRed,valueGreen,valueBlue)" - system color name
				"syscolor(COLORNAME Syscolor)" - no color "none"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern
				value="rgb\(&n255;,&n255;,&n255;\)|syscolor\([A-Z0-9]* SysColor\)|none" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="unsigned31BitInt">
		<xs:restriction base="xs:unsignedInt">
			<xs:maxInclusive value="2147483647"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="percent">
		<xs:restriction base="xs:short">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="options">
		<xs:annotation>
			<xs:documentation>different selectable options encoded by a number from 0 to x (in order of the options in the combobox in Visu+ development environment)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:short">
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="linejoinType">
		<xs:annotation>
			<xs:documentation>linejoin style: round - miter - bevel</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="round" />
			<xs:enumeration value="miter" />
			<xs:enumeration value="bevel" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="linecapType">
		<xs:annotation>
			<xs:documentation>linecap style: butt - round - square</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="butt">
			</xs:enumeration>
			<xs:enumeration value="round" />
			<xs:enumeration value="square" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="strokeDasharryType">
		<xs:annotation>
			<xs:documentation>line style: dashed "2,2" - dotted "1,1" - dash,
				dot,... "2,1" - dash, dot, dot,... "2,1,1"
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="2,2" /> <!-- dashed -->
			<xs:enumeration value="1,1" /> <!-- dotted -->
			<xs:enumeration value="2,1" /> <!-- dash, dot, dash, dot,... -->
			<xs:enumeration value="2,1,1" /> <!-- dash, dot, dot, dash, dot, dot,... -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="hatchType">
		<xs:annotation>
			<xs:documentation>hatching style: different styles numbered from 0 to
				22, for no hatching style leave attribute out</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="hatch\(2[0-2]\)|hatch\(1?[0-9]\)" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="styleType">
		<xs:annotation>
			<xs:documentation>orientation of static image: stretched "2" - middle
				"3" - top-middle "26" - bottom middle "42" - middle-left "74" -
				top-left "82" - bottom-left "98" - middle-right "138" - top-right
				""146" - bottom-right "162" - add 4 to value for fixed proportions</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="2" /> <!-- stretched -->
			<xs:enumeration value="3" /> <!-- middle -->
			<xs:enumeration value="6" /> <!-- stretched, proportions fixed -->
			<xs:enumeration value="7" /> <!-- middle, proportions fixed -->
			<xs:enumeration value="26" /> <!-- top-middle -->
			<xs:enumeration value="30" /> <!-- top-middle, proportions fixed -->
			<xs:enumeration value="42" /> <!-- bottom-middle -->
			<xs:enumeration value="46" /> <!-- bottom-middle, proportions fixed -->
			<xs:enumeration value="74" /> <!-- middle-left -->
			<xs:enumeration value="78" /> <!-- middle-left, proportions fixed -->
			<xs:enumeration value="82" /> <!-- top-left -->
			<xs:enumeration value="86" /> <!-- top-left, proportions fixed -->
			<xs:enumeration value="98" /> <!-- bottom-left -->
			<xs:enumeration value="102" /> <!-- bottom-left, proportions fixed -->
			<xs:enumeration value="138" /> <!-- middle-right -->
			<xs:enumeration value="142" /> <!-- middle-right, proportions fixed -->
			<xs:enumeration value="146" /> <!-- top-right -->
			<xs:enumeration value="150" /> <!-- top-right, proportions fixed -->
			<xs:enumeration value="162" /> <!-- bottom-right -->
			<xs:enumeration value="166" /> <!-- bottom-right, proportions fixed -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="accessLevelType">
		<xs:annotation>
			<xs:documentation>user acess level: user level 0-8 - admin level
				"1023" - developer level "1024"</xs:documentation>
