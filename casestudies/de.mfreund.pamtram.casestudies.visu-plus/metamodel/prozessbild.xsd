			<xs:simpleContent>
				<xs:extension base="xs:unsignedByte">
					<xs:attribute ref="grad" />
					<xs:attribute ref="gcolor" />
					<xs:attribute ref="ImageID" />
					<xs:attribute ref="Style" />
					<xs:attribute ref="XImage" />
					<xs:attribute ref="YImage" />
					<xs:attribute ref="tcolor" />
					<xs:attribute ref="fill" />
