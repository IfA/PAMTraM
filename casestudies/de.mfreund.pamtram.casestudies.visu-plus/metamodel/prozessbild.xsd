			<xs:attribute ref="spin" />
			<xs:attribute ref="edit" />
			<xs:attribute ref="promptpad" />
			<xs:attribute ref="fmtscale" />
			<xs:attribute ref="fmtvar" />
			<xs:attribute ref="Units" />
			<xs:attribute ref="SpinSize" />
			<xs:attribute ref="SpinWidth" />
			<xs:attribute ref="SpinHeight" />
			<xs:attribute ref="ShowDisplay" />
			<xs:attribute ref="TimeToWait" />
			<xs:attribute ref="HorizSpin" />
			<xs:attribute ref="EditingPassword" />
			<xs:attribute ref="HasList" />
			<xs:attribute ref="ShowControlList" />
			<xs:attribute ref="ListData" />
			<xs:attribute ref="SortItems" />
			<xs:attribute ref="InvertSelection" />
			<xs:attribute ref="bottom" />
			<xs:attribute ref="FlatTab" />
			<xs:attribute ref="ImageList" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="execution">
		<xs:annotation>
			<xs:documentation>execution settings of an "Edit"-object</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute ref="var" />
			<xs:attribute ref="varmin" />
			<xs:attribute ref="varmax" />
