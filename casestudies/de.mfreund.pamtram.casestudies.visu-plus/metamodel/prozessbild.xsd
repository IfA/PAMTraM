				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="g">
		<xs:annotation>
			<xs:documentation>general settings of an "group"-object (instead of element "type")</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute ref="id" />
			<xs:attribute ref="x" />
			<xs:attribute ref="y" />
			<xs:attribute ref="width" />
			<xs:attribute ref="height" />
			<xs:attribute ref="ox" />
			<xs:attribute ref="oy" />
			<xs:attribute ref="owidth" />
			<xs:attribute ref="oheight" />
			<xs:attribute ref="Border" />
		</xs:complexType>
	</xs:element>

	<xs:element name="editbox">
		<xs:annotation>
			<xs:documentation>general settings of an "Edit"-object (instead of element "type")</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute ref="x" />
			<xs:attribute ref="y" />
			<xs:attribute ref="width" />
			<xs:attribute ref="height" />
			<xs:attribute ref="hatch" />
			<xs:attribute ref="fill" />
			<xs:attribute ref="stroke-dasharray" />
			<xs:attribute ref="stroke" />
