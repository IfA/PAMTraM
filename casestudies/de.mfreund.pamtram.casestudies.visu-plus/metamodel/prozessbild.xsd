		</xs:simpleType>
	</xs:attribute>
	

	<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++
	++++++++++++++++++ definition of groups ++++++++++++++++++
	++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	
	<xs:group name="groupChild">
		<xs:sequence>
			<xs:element name="type" minOccurs="1" maxOccurs="2">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="groupChildEnum">
							<xs:attribute ref="x" />
							<xs:attribute ref="y" />
							<xs:attribute ref="width" />
							<xs:attribute ref="height" />
		 					<xs:attribute ref="chFigure"/>
							<xs:attribute ref="rx" />
							<xs:attribute ref="ry" />
							<xs:attribute ref="rxa" />
							<xs:attribute ref="rya" />
							<xs:attribute ref="ArrowHeight" />
							<xs:attribute ref="hatch" />
							<xs:attribute ref="fill" />
							<xs:attribute ref="stroke-dasharray" />
							<xs:attribute ref="stroke" />
							<xs:attribute ref="stroke-width" />
							<xs:attribute ref="stroke-linecap" />
							<xs:attribute ref="stroke-linejoin" />
							<xs:attribute ref="l3D" />
							<xs:attribute ref="p3D" />
							<xs:attribute ref="KAR"/>
							<xs:attribute ref="Border" />
							<xs:attribute ref="ImageDown" />
							<xs:attribute ref="ImageUp" />
							<xs:attribute ref="ImageChecked" />
							<xs:attribute ref="ImageDisable" />
							<xs:attribute ref="BtnImageOverlap" />
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="g" minOccurs="0" maxOccurs="1"/> <!-- nur bei "group"-object -->
			<xs:element ref="editbox" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Edit"-object -->
			<xs:element ref="TabObj" minOccurs="0" maxOccurs="1"/> <!-- nur bei "TabObj"-object -->
			<xs:element ref="style" minOccurs="0" maxOccurs="1"/> <!--  bei "Edit"- und "TabObj"-object -->
			<xs:element ref="border" minOccurs="0" maxOccurs="1"/> <!--  bei "Edit"- und "TabObj"-object -->
			<xs:element ref="execution" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Edit"-object -->
			<xs:element ref="Screens" minOccurs="0" maxOccurs="1"/> <!-- nur bei "TabObj"-object -->
			<xs:element ref="ExtFill" minOccurs="0" maxOccurs="1"/>
			<xs:element name="childs" minOccurs="0" maxOccurs="1"> <!-- nur bei "group"-object -->
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded" minOccurs="1">
						<xs:element name="child">
							<xs:complexType>
								<xs:sequence>
									<xs:group ref="groupChild" minOccurs="1" maxOccurs="1"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="Border" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Button"-object -->
			<xs:element ref="Mechanic" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Button"-object -->
			<xs:element ref="Commands" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Button"-object -->
			<xs:element ref="CommandsWhileDown" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Button"-object -->
			<xs:element ref="CommandsOnDown" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Button"-object -->
			<xs:element ref="KeyCode" minOccurs="0" maxOccurs="1"/> <!-- nur bei "Button"-object -->
			<xs:element ref="Name" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="text" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="Shadow" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="Rotation" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="RunSel" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="ListCommands" minOccurs="0" maxOccurs="1"/>
			<xs:group ref="animations" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:group>
	
	<xs:group name="animations">
		<xs:sequence>
			<xs:element ref="Visible" minOccurs="1" maxOccurs="1"/>
