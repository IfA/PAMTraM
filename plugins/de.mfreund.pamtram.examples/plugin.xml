         </fileToOpen>
      </example>
      <example
            wizardID="de.mfreund.pamtram.examples.visu-plus">
         <projectDescriptor
               contentURI="zips/visu-plus.zip"
               description="This casestudy demonstrates a transformation from &apos;STGML&apos; to &apos;Visu+&apos;."
