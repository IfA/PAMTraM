		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="textAnchorType">
		<xs:annotation>
			<xs:documentation>text position</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="middle" />
