            finalPerspective="de.mfreund.pamtram.pamtramPerspective"
            icon="icons/project.png"
            id="de.mfreund.pamtram.examples.visu-plus"
            name="STGML -&gt; Visu+"
            project="true">
      </wizard>
      <category
            id="de.mfreund.pamtram.examples.category"
            name="PAMTraM"
            parentCategory="org.eclipse.ui.Examples">
      </category>
   </extension>
   <extension
         point="org.eclipse.emf.common.ui.examples">
      <example
            wizardID="de.mfreund.pamtram.examples.autoprobe">
         <projectDescriptor
               contentURI="zips/autoprobe.zip"
               description="This casestudy demonstrates the results from the &apos;AutoProBe&apos; project."
               name="de.mfreund.pamtram.casestudies.autoprobe">
         </projectDescriptor>
         <fileToOpen
               editorID="pamtram.presentation.PamtramEditorID"
               location="de.mfreund.pamtram.casestudies.autoprobe/Pamtram/STGML2Movisa_enhanced.pamtram">
         </fileToOpen>
      </example>
      <example
            wizardID="de.mfreund.pamtram.examples.disl-movisa">
         <projectDescriptor
               contentURI="zips/disl-movisa.zip"
               description="This casestudy demonstrates a simple transformation from &apos;DISL&apos; to &apos;Movisa&apos;."
               name="de.mfreund.pamtram.casestudies.disl-movisa">
         </projectDescriptor>
         <fileToOpen
               editorID="pamtram.presentation.PamtramEditorID"
               location="de.mfreund.pamtram.casestudies.disl-movisa/Pamtram/Disl2Movisa.pamtram">
