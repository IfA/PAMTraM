			<xs:element ref="EGList" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="MX" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="MY" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="Scale" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="Rot" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="SX" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="SY" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="EX" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="EY" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="FC" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="FCList" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="BList" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="BmpList" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="LT" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="RCX" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="RCY" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="CM" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:group>
	

	<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++
	+++++++++++++ definition of simple elements ++++++++++++++
	++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->

	<xs:element name="object" type="objectEnum">
		<xs:annotation>
			<xs:documentation>defines the type of graphical object</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="border">
