            icon="icons/project.png"
            id="de.mfreund.pamtram.examples.disl-movisa"
            name="Disl -&gt; Movisa"
            project="true">
      </wizard>
      <wizard
            category="org.eclipse.ui.Examples/de.mfreund.pamtram.examples.category"
